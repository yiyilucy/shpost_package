<%- model_class = ReturnResult -%>
<div class="page-header">
  <h1>退件</h1>
</div>
<div>
	<table>
		<tr>
			<td><%= label_tag 'name', '约投号码:', class: 'control-group' %></td>
 			<td><%= text_field_tag 'registration_no', nil, class: "controls" %></td>
 			<td><input type="button" id="setreason" value="确定" class='btn btn-primary' ></td>
      <td><font size="5"><%= label_tag 'name2', '已扫件数:', class: 'control-group' %></font></td>
      <td><font size="5"><div id = 'return_num'>0</div></font></td>
 		</tr>
		<tr>
 			<td valign="top"><%= label_tag 'name1', '退件理由设置:', class: 'control-group' %></td>
 			<td><%= select_tag "return_reason", options_for_select(ReturnReason.reason_select_options(current_user.unit_id)), size:ReturnReason.accessible_by(current_ability).count %></td>
 		</tr>
  </table>
</div>
<div id="out_query_results">
  <%= render :partial => 'return_results/return_scan_form' %>
</div>
